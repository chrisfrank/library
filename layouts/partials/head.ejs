<head>
  <meta charset="utf-8">
  <meta id="meta" name="viewport" content="width=device-width, initial-scale=1.0" />
  <title><%= title %></title>
  <!-- HTML5 shim, for IE6-8 support of HTML elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->

  <% if (locals.inlineAssets) { %>
    <style type="text/css"><%- inlineAssets['css'] %></style>
    <link rel="shortcut icon" href="<%- inlineAssets['branding.favicon'] %>" type="image/x-icon" />
  <% } else { %>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.js"></script>
    <script src="/assets/scripts/main.js" charset="utf-8"></script>
    <!-- styles -->
    <link rel="shortcut icon" href="<%- template('branding.favicon') %>" type="image/x-icon" />
    <link href="/assets/css/style.css" rel="stylesheet">
  <% } %>
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
</head>
